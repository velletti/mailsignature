{
    "name": "velletti/mailsignature",
    "type": "typo3-cms-extension",
    "description": "Velletti  Mailsignature store Import (eMail signatures) as database entries and use them with viewhelper in emails",
    "homepage": "https://github.com/velletti/mailsignature/",
    "license": [
        "GPL-2.0+"
    ],
    "keywords": ["TYPO3 CMS", "mailsignature", "Frontend"],
    "require": {
        "typo3/cms-core": "^10.4.0 || ^11.5"
    },
    "autoload": {
        "psr-4": {
            "Velletti\\Mailsignature\\": "Classes"
        }
    },
    "config": {
        "vendor-dir": ".Build/vendor",
        "bin-dir": ".Build/bin"
    },
    "extra": {
        "typo3/cms": {
            "web-dir": ".Build/Web",
            "extension-key": "mailsignature"
        }
    }
}
